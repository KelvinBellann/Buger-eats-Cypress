{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "cypress",
      "node",
      "cypress-file-upload",
      "@badeball/cypress-cucumber-preprocessor"
    ],
    "resolveJsonModule": true,
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": "."
  },
  "include": ["cypress/**/*.ts", "cypress/**/*.d.ts", "cypress.config.ts"]
}
